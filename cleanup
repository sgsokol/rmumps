#make -C inst clean
for f in src/lib/scotch*/src; do
   make -C $f realclean
done
find \( -name '*.bck' -o -name '*.o' -o -name '*.so' -o -name .RData -o -name .Rhistory -o -name '*.dll' -o -name '*.a'  -o -name '*.mod' \) -exec rm -f {} \;
find \( -name 'tmp' -o -name 'build' \) -exec rm -fr {} \;
